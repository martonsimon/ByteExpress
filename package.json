{
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^14.11.2",
    "concurrently": "^8.1.0",
    "gts": "^3.1.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "start": "nodemon build/index.js",
    "build": "tsc -p .",
